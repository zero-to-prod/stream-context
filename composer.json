  "autoload": {
    "psr-4": {
      "Zerotoprod\\StreamContext\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  }
